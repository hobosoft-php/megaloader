{
    "name": "hobosoft/megaloader",
    "version": "1.0.0",
    "description": "PHP mega loader",
    "minimum-stability": "dev",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hobosoft\\MegaLoader\\": "src/",
            "Hobosoft\\MegaLoader\\Tests\\": "tests/"
        }
    },
    "require": {
        "psr/log": "^3.0@dev",
        "psr/simple-cache": "^3.0@dev",
        "hobosoft/boot": "*",
        "hobosoft/config": "*",
        "hobosoft/fileloaders": "*",
        "hobosoft/finders": "*",
        "symfony/yaml": "7.2.x-dev",
        "ext-apcu": "*",
        "filp/whoops": "^2.15"
    },
    "require-dev": {
        "laminas/laminas-loader": "2.11.x-dev",
        "phpunit/php-code-coverage": "*",
        "smeghead/php-class-diagram": "*",
        "tracy/tracy": "*"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
